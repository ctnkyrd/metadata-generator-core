<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

  <connectionStrings>

    <add name="PostgreSQLConnection" connectionString="PORT=5432;DATABASE=tues;HOST=localhost;PASSWORD=kalman;USER ID=postgres"/>
    
  </connectionStrings>
  
  <appSettings>

    <add key="parameters" value="parameters.json"/>
    
    <!--katmana ilişkin tablo adı-->
    <add key="metaTableName" value="anit" />
    
    <!--tablodaki aktif kayıtları bulmak için-->
    <add key="tableCriteria" value="GEO_DURUM = true" />

    <!--guid kolonu ismi-->
    <add key="guidColumn" value="GUID" />
    
    <!--kurum yetkili kişisinin adı-->
    <add key="organizationEmail" value="yasingulbay@gmail.com" />
    <!--Kurum adı-->
    <add key="organizationName" value="Kültür ve Turizm Bakanlığı" />
    <!--örn: E:\netcadweb\metadata için "E:\\netcadweb\\metadata" yazınız-->
    <add key="metaDataFolder" value="GENERATEDXML" />
    <!--SQL Server için Bağlantı Cümleciği-->
    <add key="SQLServer" value="Data Source=192.168.30.138;Initial Catalog=TUES;User id=sa;Password=Ankara123;" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
    
    <!--PostgreSQL için Bağlantı Cümleciği-->
    <add key="PostgreSQLConnection" value="PORT=5432;DATABASE=tues;HOST=localhost;PASSWORD=kalman;USER ID=postgres"/>
    
    <!--keyword kolon isimleri-->
    <add key="keywords" value="ADI;PAFTA"/>
    
    <!--topic category-->
    <add key="topicCategory" value="location"/>

    <!--online source wms/wfs ';' ile ekleyiniz-->
    <add key="onlineSources" value="https://gis-prod-api.csb.gov.tr/sit_wms;https://gis-prod-api.csb.gov.tr/sit_wfs"/>
    
  </appSettings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>